<notificationPop>:
    title: 'Taboo Word Find'
    size_hint:(0.8,0.8)
    BoxLayout:
        size_hint:(0.8,0.8)
        orientation: 'vertical'
        size_hint: (0.6, 0.6)
        spacing: 20
        pos_hint:{'center_y': .5, 'center_x': 0.5}
        canvas.before:
            Color:
                rgb: (255, 255, 255)
            Rectangle:
                pos: self.pos
                size: self.size
