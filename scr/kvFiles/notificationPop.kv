<notificationPop>:
    title: 'Notifications'
    size_hint:(0.8,0.8)
    canvas.before:
        Color:
            rgb: (255, 255, 255)
        Rectangle:
            pos: self.pos
            size: self.size
    BoxLayout:
        size_hint:(0.8,0.8)
        orientation: 'vertical'
        size_hint: (0.6, 0.6)
        spacing: 20
        pos_hint:{'center_y': .5, 'center_x': 0.5}

        WarningLabel:
            size_hint_y: .7
            pos_hint:{'center_y': .5, 'center_x': 0.5}
            font_size: 30
            id: messageShow

        BoxLayout:
            size_hint: (0.3,.2)
            pos_hint:{'center_y': .5, 'center_x': 0.5}
            spacing: 20
            GrayButton:
                text: "Dismiss"
                on_press: root.dismissPop()


