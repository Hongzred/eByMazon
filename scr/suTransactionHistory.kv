<suTransactionRow@BoxLayout>:
    itemID: ''
    buyerID: ''
    price: ''
    numDeal: ''
    shipping_status: ''
    date: ''
    BoxLayout:
        GridLayout:
            cols: 6
            Label:
                text: root.itemID
                color: 0,0,1,1
            Label:
                text: root.buyerID
                color: 0,0,1,1
            Label:
                text: root.price
                color: 0,0,1,1
            Label:
                text: root.numDeal
                color: 0,0,1,1
            Label:
                text: root.shipping_status
                color: 0,0,1,1
            Label:
                text: root.date
                color: 0,0,1,1

<suTransaction>:
    # name: "GUapplication"
    BoxLayout:
        orientation: "vertical"
        spacing: 20
        BoxLayout:
            orientation: 'vertical'
            size_hint_y: .2
            BoxLayout:
                BoldLabel:
                    # background_color: [2.37, 1.13, .68, 0.7]
                    text: 'Transactions'
                    color: 2.37, 1.13, .68, 0.7
                    canvas.before:
                        Color:
                            rgb: (0, 255, 255,1)
                        Rectangle:
                            pos: self.pos
                            size: self.size
            BoxLayout:
                orientation: 'horizontal'
                BoldLabel:
                    text: 'Item ID'
                BoldLabel:
                    text: 'buyerID'
                BoldLabel:
                    text: 'price'
                BoldLabel:
                    text: 'numDeal'
                BoldLabel:
                    text: 'shipped?'
                BoldLabel:
                    text: 'Date'
        RecycleView:
            size_hint_y: .7
            id: transactions
            data:[]
            viewclass: 'suTransactionRow'
            VRVL:
                default_size: None, dp(20)
        BoxLayout:
            size_hint_y: .1
            GrayButton:
                text:"home"
                size: 150, 100
                on_press: root.tohome()